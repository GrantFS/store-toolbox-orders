{
  "name": "@grantfs/store-toolbox-orders",
  "version": "1.1.0",
  "description": "Order processing utilities for Store Toolbox - ID generation, VAT calculations, UK address normalization",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js"
    },
    "./dynamodb": {
      "types": "./dist/dynamodb.d.ts",
      "import": "./dist/dynamodb.mjs",
      "require": "./dist/dynamodb.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ecommerce",
    "store",
    "toolbox",
    "orders",
    "vat",
    "uk"
  ],
  "author": "Grant Pearce",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrantFS/store-toolbox-orders.git"
  },
  "bugs": {
    "url": "https://github.com/GrantFS/store-toolbox-orders/issues"
  },
  "homepage": "https://github.com/GrantFS/store-toolbox-orders#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "peerDependencies": {
    "@grantfs/store-toolbox-core": "^1.2.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0"
  },
  "devDependencies": {
    "@aws-sdk/lib-dynamodb": "^3.726.1",
    "@grantfs/store-toolbox-core": "^1.2.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
